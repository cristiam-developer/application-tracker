DATABASE_URL="file:./dev.db"

# Google OAuth (Gmail integration)
# Create at: https://console.cloud.google.com/ → APIs & Services → Credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=

# Job Search APIs (all optional — app works without them)
JSEARCH_API_KEY=
ADZUNA_APP_ID=
ADZUNA_APP_KEY=

# Playwright web scraper (set to "true" to enable Indeed/Glassdoor scraping)
ENABLE_SCRAPER=
